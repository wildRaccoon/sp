{
  "sp.auth": {
    "hash": {
      "algorithm": "sha256"
    },
    "authenticate": {
      "sessionExpiredInSec": 60,
      "renewExpiredInSec": 300
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "SpAuthDb": "Host=db;Database=SpAuthDataBase;Username=postgres;Password=mypass"
  }
}
